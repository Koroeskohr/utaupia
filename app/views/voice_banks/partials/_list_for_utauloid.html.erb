<div class="utauloid-show-voicebanks-list">
	<div class="utauloid-show-voicebanks-list-title">
		<h2><%= t('voicebank') %></h2>
	</div>

	<% if !current_user.blank? && current_user.id == utau.creator_id && utau.creator_is_user == true %>
	<div class="utauloid-show-voicebanks-list-add-link">
		<%= link_to t('add'), new_voice_bank_path(utau) %>
	</div>
	<% end %>
	<% if voice_banks.any? %>
		<% voice_banks.each do |vb| %>
		  <% if !vb.new_record? %>
		    <%= render partial: 'voice_banks/partials/voice_bank', locals: { utau: utau, voice_bank: vb } %>
		  <% end %>
		<% end %>
	<% else %>
	<div class="utauloid-show-voicebanks-list-item">
	  <div class="utauloid-show-voicebanks-list-item-download">
			<p>This UTAUloid doesn't have any voicebank at the moment.</p>
		</div>
	</div>
	<% end %>
</div>
